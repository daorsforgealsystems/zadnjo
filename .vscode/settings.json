{
  "deno.path": "C:\\Users\\User\\AppData\\Local\\deno\\bin\\deno.exe",
  "deno.enable": true,
  "deno.enablePaths": [
    "netlify/edge-functions"
  ],
  "deno.importMap": ".netlify/edge-functions-import-map.json",
  "zencoder.mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest",
        "--isolated",
        "--headless",
        "--browser",
        "firefox",
        "--user-agent",
        "\"Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/125.0.6422.112 Safari/537.36\""
      ]
    }
  },
  "codacy.cli.cliVersion": "codacy-cli-v2",
  "extension.prophet.sitecontext.selectedSiteID": "",
  "deno.unstable": true
}