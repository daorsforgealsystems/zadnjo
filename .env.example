# Frontend Environment Variables

# API Endpoints
VITE_API_BASE_URL=http://localhost:8080
VITE_USER_SERVICE_URL=http://localhost:4001/api
VITE_INVENTORY_SERVICE_URL=http://localhost:8000
VITE_ROUTING_SERVICE_URL=http://localhost:8002
VITE_GEOLOCATION_SERVICE_URL=http://localhost:8003
VITE_NOTIFICATION_SERVICE_URL=http://localhost:8004

# Authentication
VITE_AUTH_PROVIDER=supabase
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Feature Flags
VITE_ENABLE_ANIMATIONS=true
VITE_ENABLE_CUSTOMIZATION=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_REAL_TIME=true

# Development Settings
VITE_DEV_MODE=true
VITE_MOCK_API=false
VITE_DEBUG_LAYOUT=false

# Theme Configuration
VITE_DEFAULT_THEME=light
VITE_DEFAULT_PRIMARY_COLOR=#3b82f6

# Performance Settings
VITE_ANIMATION_DURATION=300
VITE_DEBOUNCE_DELAY=1000
VITE_AUTO_SAVE_INTERVAL=30000